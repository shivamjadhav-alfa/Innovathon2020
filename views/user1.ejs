<h1 style="text-align: center;">REGISTRATION FORM</h1>
  <form id="#regform" action="/reg1" method="POST" style="text-align: center;margin-top:50px">
    <div class="control-group">
      <div class="controls">
        <input class="" type="text" id="Team-name"
          style="text-align:center;width:40%;height:45px;color:black;margin-bottom:25px;margin-right:2% ;"
          name="TeamName" placeholder="* Teamname ..." required />
      </div>
    </div>


    <div class="control-group">
      <div class="controls">
        <input class="" type="text" id="name"
          style="text-align:center;border-radius: 2%;width:40%;color:black;height:45px;margin-bottom:25px;margin-right:2% ;"
          name="name1" placeholder="* participant 1 name..." required />


      </div>
    </div>

    <div class="control-group">
      <div class="controls">
        <input class="" type="text" id="age"
          style="text-align:center;color:black;border-radius: 2%;width:40%;height:45px;margin-bottom:25px;margin-right:2% "
          name="age1" placeholder="* enter your age." required />


      </div>
    </div>
    <div class="control-group">
      <div class="rs-select2 js-select-simple select--no-search">
        <select id="gender" name="gender1"
          style="text-align:center;border-radius: 2%;width:40%;color:black;height:45px;border-radius: 10px;margin-bottom:25px;margin-right: 2%;">
          <option disabled="disabled" selected="selected">Gender</option>
          <option>Male</option>
          <option>Female</option>
          <option>Other</option>
        </select>

        <div class="select-dropdown"></div>
      </div>

    </div>
    <div class="control-group">
      <div class="rs-select2 js-select-simple select--no-search">
        <select id="Tshirtsize" name="Tshirtsize1"
          style="border-radius:2%;text-align:center;width:40%;color:black;height:45px;margin-bottom:25px;margin-right: 2%;">
          <option disabled="disabled" selected="selected">Tshirtsize</option>
          <option>S</option>
          <option>M</option>
          <option>L</option>
          <option>Xl</option>
          <option>XXL</option>
        </select>


        <div class="select-dropdown"></div>
      </div>

    </div>
    <div class="control-group">
      <div class="controls">
        <input class="" type="email"
          style="text-align:center;color:black;width:40%;height:45px;margin-bottom:25px;margin-right: 2%;" name="email1"
          id="email" placeholder="* Your email..." required />


      </div>
    </div>
    <div class="control-group">
      <div class="controls">
        <input class="" type="text"
          style="text-align:center;color:black;width:40%;height:45px;margin-bottom:25px;margin-right: 2%;"
          name="phoneNo" id="phno" placeholder="* Your phone number required..." required />
      </div>
    </div>
    </div>
    <div class="control-group">
      <div class="controls">
        <button type="submit" id='sub'
          style="text-align:center;width:10%;height:10%;color:black;height:4em;margin-bottom:25px;color:black;border-radius: 10px;">Submit</button>
      </div>
    </div>
    <style>
      #sub:hover {
        background-color: green;
        color: black;
      }

      #phno:hover,
      #email:hover,
      #age:hover,
      #gender:hover,
      #name:hover,
      #team-name:hover {
        border: 2px green solid;
        width: 15%;
      }
    </style>
